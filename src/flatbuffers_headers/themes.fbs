namespace steamrot.themes; 

struct Color {
    r:ubyte;
    g:ubyte;
    b:ubyte;
    a:ubyte;
  }
table PanelStyle {
    background_color: Color;
    border_color: Color;
    border_thickness: float;
    radius_resolution : int;
  }

table ButtonStyle {
  background_color: Color;
  text_color: Color;
  border_color: Color;
  }

table UIObjects {
    panel_style: PanelStyle;
    button_style: ButtonStyle;

  }
root_type UIObjects;
