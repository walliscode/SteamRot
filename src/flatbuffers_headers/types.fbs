// general types to use in other schemas

table Vector2fData {
    x: float;
    y: float;
  }


struct ColorData{
    r:uint8;
    g:uint8;
    b:uint8;
    a:uint8;
  }


table Vertex {
  position: Vector2fData(required);
  color: ColorData (required);
}


table Triangle {
vertices: [Vertex] (required);
  }
